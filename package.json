{
  "name": "google-maps-geocoding-mcp",
  "version": "1.0.2",
  "description": "MCP server for Google Maps Geocoding API",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "google-maps-geocoding-mcp": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc && chmod +x dist/index.js",
    "build:watch": "tsc --watch --preserveWatchOutput",
    "clean": "rm -rf dist",
    "prepublishOnly": "pnpm run clean && pnpm run build && pnpm run test:run",
    "test": "vitest",
    "test:run": "vitest --run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "precommit": "pnpm run lint && pnpm run type-check && pnpm run format",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "keywords": [
    "mcp",
    "google-maps",
    "geocoding",
    "claude",
    "model-context-protocol"
  ],
  "author": "Kevin Wu",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kevinwuhoo/google-maps-geocoding-mcp.git"
  },
  "bugs": {
    "url": "https://github.com/kevinwuhoo/google-maps-geocoding-mcp/issues"
  },
  "homepage": "https://github.com/kevinwuhoo/google-maps-geocoding-mcp#readme",
  "files": [
    "dist/**/*",
    "README.md",
    "CONTRIBUTING.md",
    "LICENSE"
  ],
  "dependencies": {
    "@googlemaps/google-maps-services-js": "^3.4.2",
    "@modelcontextprotocol/sdk": "^1.17.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@tsconfig/node24": "^24.0.1",
    "@types/node": "^24.3.0",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.33.0",
    "prettier": "^3.6.2",
    "tsx": "^4.20.4",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.40.0",
    "vite": "^7.1.3",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
